{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "jsx": "preserve",
    "allowJs": true,
    "types": ["vite/client"],
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@composables/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}